<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.springframework</groupId>
  <artifactId>messaging-jms</artifactId>
  <version>0.1.0</version>
  
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.8</java.version>
	</properties>
	
	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-activemq</artifactId>
			<version>1.4.0.RELEASE</version>
		</dependency>	
        <dependency>
            <groupId>org.apache.activemq</groupId>
            <artifactId>activemq-broker</artifactId>
            <version>5.15.0</version>
        </dependency>
        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-databind</artifactId>
            <version>2.9.10.7</version>
        </dependency>
	</dependencies>
	
	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

 	<repositories>
        <repository>
            <id>axiom-nexus</id>
            <url>http://isgnexus.deere.com/nexus/content/groups/public</url>
        </repository>
        <repository>
            <id>central-DeereProxy</id>
            <name>Maven Central Deere Proxy</name>
            <url>http://isgnexus.deere.com/nexus/content/repositories/central/</url>
        </repository>
        <repository>
            <id>REPO.DEERE.COM</id>
            <name>Maven Deere Repo</name>
            <url>http://isgnexus.deere.com/nexus/content/repositories/repo.deere.com/</url>
        </repository>
    </repositories>

    <pluginRepositories>
        <pluginRepository>
            <id>axiom-nexus</id>
            <url>http://isgnexus.deere.com/nexus/content/groups/public</url>
        </pluginRepository>
        <pluginRepository>
            <id>central-DeereProxy</id>
            <name>Maven Central Deere Proxy</name>
            <url>http://isgnexus.deere.com/nexus/content/repositories/central/</url>
        </pluginRepository>
    </pluginRepositories>
    
</project>